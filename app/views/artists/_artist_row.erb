<li>
  <%= link_to(artist_path(artist)) do %>
    <div class='ind-artist-name'>
      <%= artist.name %>
    </div>

    <div class='artist-artworks'>
      <% artworks.each do |artwork| %>
        <% if artwork.thumbnail.presence %>
          <%= image_tag artwork.thumbnail %>
        <% end %>
      <% end %>
    </div>
  <% end %>
</li>
